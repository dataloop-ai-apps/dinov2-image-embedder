{
  "name": "dino-v2-adapter",
  "displayName": "DINOv2 Adapter",
  "description": "DINOv2 image embedder",
  "version": "0.1.9",
  "scope": "public",
  "attributes": {
    "Category": "Model",
    "Provider": "Meta",
    "Media Type": [
      "Image"
    ],
    "Computer Vision": [
      "Embeddings"
    ],
    "License": "Apache 2.0"
  },
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/dino-v2-adapter",
    "gitTag": "0.1.9"
  },
  "components": {
    "modules": [
      {
        "name": "dino_v2_adapter",
        "entryPoint": "adapter.py",
        "className": "DINOv2Adapter",
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "embed_items",
            "description": "Extract DINOv2 features on a single item",
            "input": [
              {
                "type": "Item",
                "name": "item"
              }
            ],
            "output": [
              {
                "type": "Item",
                "name": "item"
              },
              {
                "type": "Json",
                "name": "json",
                "description": "Embeddings of items."
              }
            ],
            "displayName": "DINOv2 Features"
          },
          {
            "name": "embed_dataset",
            "description": "Extract DINOv2 features on a dataset",
            "input": [
              {
                "type": "Dataset",
                "name": "dataset"
              },
              {
                "type": "Json",
                "name": "filters"
              }
            ],
            "output": [],
            "displayName": "DINOv2 Dataset Features"
          }
        ]
      }
    ],
    "computeConfigs": [
      {
        "name": "dino-v2-cpu",
        "moduleName": "dino_v2_adapter",
        "runtime": {
          "podType": "regular-m",
          "concurrency": 10,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/apps/dinov2-adapter:0.1.0",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 100
          }
        },
        "executionTimeout": 3600
      }
    ],
    "models": [
      {
        "name": "dino-v2",
        "moduleName": "dino_v2_adapter",
        "status": "pre-trained",
        "configuration": {
          "batch_size": 10,
          "model_name": "dinov2_vits14"
        }
      }
    ]
  }
}