{
  "name": "dino-v2-adapter",
  "displayName": "DINOv2 Adapter",
  "description": "DINOv2 image embedder",
  "version": "0.1.5",
  "scope": "public",
  "attributes": {
    "Category": "Model",
    "Provider": "Meta",
    "Media Type": ["Image"],
    "Computer Vision": ["Embeddings"],
    "License": "Apache 2.0"
  },
    "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/dino-v2-adapter",
    "gitTag": "0.1.5"
  },
  "components": {

    "modules": [
      {
        "name": "dino_v2_adapter",
        "entryPoint": "adapter.py",
        "className": "DINOv2Adapter",
        "functions": [
          {
            "name": "extract_item",
            "description": "Extract CLIP features on a single item",
            "input": [
              {
                "type": "Item",
                "name": "item"
              }
            ],
            "output": [
              {
                "type": "Item",
                "name": "item"
              }
            ],
            "displayName": "Clip Features"
          },
          {
            "name": "extract_dataset",
            "description": "Extract DINOv2 features on a dataset",
            "input": [
              {
                "type": "Dataset",
                "name": "dataset"
              },
              {
                "type": "Json",
                "name": "query"
              }
            ],
            "output": [
              {
                "type": "Dataset",
                "name": "dataset"
              }
            ],
            "displayName": "DINOv2 Dataset Features"
          }
        ]
      }
    ],
    "services": [
      {
        "name": "dino-v2-extraction",
        "moduleName": "dino_v2_extract_features",
        "runtime": {
          "podType": "regular-m",
          "concurrency": 10,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/cpu/dino-v2-search:0.1.0",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 100
          }
        },
        "executionTimeout": 360000
      }
    ],
    "models": [
      {
        "name": "dino-v2",
        "moduleName": "dino_v2_adapter",
        "configuration": {
          "batch_size": 10,
          "model_name": "dinov2_vits14"
        }
      }
    ]
  }
}