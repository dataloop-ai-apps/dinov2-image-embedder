{
  "name": "dino-v2-adapter",
  "displayName": "DINOv2 Adapter",
  "description": "DINOv2 image embedder",
  "version": "0.1.14",
  "scope": "public",
  "attributes": {
    "Category": "Model",
    "Provider": "Meta",
    "Media Type": [
      "Image"
    ],
    "Computer Vision": [
      "Embeddings"
    ],
    "License": "Apache 2.0"
  },
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/dino-v2-adapter",
    "gitTag": "0.1.14"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "dino-v2-deploy",
        "runtime": {
          "podType": "regular-m",
          "concurrency": 10,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/apps/dinov2-adapter:0.1.0",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 100
          }
        },
        "executionTimeout": 3600
      },
      {
        "name": "dino-v2-train-evaluate",
        "runtime": {
          "podType": "gpu-t4",
          "concurrency": 10,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/apps/dinov2-adapter:0.1.0",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 100
          }
        },
        "executionTimeout": 36000
      }
    ],
    "modules": [
      {
        "name": "dino-v2-module",
        "entryPoint": "adapter.py",
        "className": "DINOv2Adapter",
        "computeConfig": "dino-v2-deploy",
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "embed_items",
            "computeConfig": "dino-v2-deploy",
            "description": "Extract DINOv2 features on a single item",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The input items for embeddings."
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The same input items for embeddings."
              },
              {
                "type": "Json",
                "name": "json",
                "description": "Embeddings of items."
              }
            ],
            "displayName": "DINOv2 Features"
          },
          {
            "name": "embed_dataset",
            "computeConfig": "dino-v2-deploy",
            "description": "Extract DINOv2 features on a dataset",
            "input": [
              {
                "type": "Dataset",
                "name": "dataset"
              },
              {
                "type": "Json",
                "name": "filters"
              }
            ],
            "output": [],
            "displayName": "DINOv2 Dataset Features"
          },
          {
            "name": "train_model",  
            "computeConfig": "dino-v2-train-evaluate",
            "input": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              }
            ],
            "output": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              }
            ],
            "displayName": "Train a Model",
            "displayIcon": "",
            "description": "Function to train DinoV2"
          },
          {
            "name": "evaluate_model",
            "computeConfig": "dino-v2-train-evaluate",
            "input": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              },
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "Dataloop Dataset Entity"
              },
              {
                "type": "Json",
                "name": "filters",
                "description": "Dataloop Filter DQL"
              }
            ],
            "output": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              },
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "Dataloop Dataset Entity"
              }
            ],
            "displayName": "Evaluate a Model",
            "displayIcon": "",
            "description": "Function to evaluate DinoV2 model performance"
          }
        ]
      }
    ],
    "models": [
      {
        "name": "dino-v2",
        "moduleName": "dino-v2-module",
        "status": "pre-trained",
        "scope": "project",
        "inputType": "image",
        "outputType": "self_supervised",
        "configuration": {
          "model_name": "dinov2_vits14",
          "embeddings_size": 384,
          "num_epochs": 100,
          "learning_rate": 0.00001,
          "weight_decay": 0.0001,
          "save_interval": 10,
          "temperature": 0.5,
          "batch_size": 8,
          "patience": 10
        },
        "description": "DINOv2 image embedder"
      }
    ]
  }
}
